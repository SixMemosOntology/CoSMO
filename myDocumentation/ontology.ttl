@prefix : <https://mangiafrangette.github.io/cosmo.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://mangiafrangette.github.io/cosmo.owl> .

<https://mangiafrangette.github.io/cosmo.owl> rdf:type owl:Ontology ;
                                               owl:versionIRI <https://mangiafrangette.github.io/cosmo.owl> ;
                                               <http://purl.org/dc/elements/1.1/creator> "Carlo Teo Pedretti"@en ,
                                                                                         "Francesca Mangialardo"@en ,
                                                                                         "Mattia Spadoni"@en ;
                                               <http://purl.org/dc/terms/title> "CoSMO - Calvino's Six Memos Ontology"@en ;
                                               <http://purl.org/vocab/frbr/core#Rights> "This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/)."@en ;
                                               rdfs:comment "CoSMO (Calvino's Six Memos Ontology) is a literary criticism based ontology that expresses in a formal way the concept behind Calvino's textual interpretation."@en ;
                                               owl:versionInfo "1.0.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/frbr/core#Rights
<http://purl.org/vocab/frbr/core#Rights> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://mangiafrangette.github.io/cosmo.owl#denotes
:denotes rdf:type owl:ObjectProperty ;
         owl:inverseOf :isDenotedBy ;
         rdfs:domain :rhetoricalFigure ;
         rdfs:range :interpretedConcept ;
         rdfs:comment "This property relates a rhetorical figure to an interpreted concept. E.g. a period that expresses mutability could be constructed using anaphora."@en ;
         rdfs:label "denotes"@en .


###  https://mangiafrangette.github.io/cosmo.owl#hasEmbodiment
:hasEmbodiment rdf:type owl:ObjectProperty ;
               owl:inverseOf :isEmbodimentOf ;
               rdfs:domain :interpretedConcept ;
               rdfs:range :fictionalCharacter ;
               rdfs:comment "This property relates an interpreted concept to a fictional character that represents it."@en ;
               rdfs:label "has embodiment"@en .


###  https://mangiafrangette.github.io/cosmo.owl#hasFictionalCharacter
:hasFictionalCharacter rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isFictionalCharacterIn ;
                       rdfs:domain :literaryText ;
                       rdfs:range :fictionalCharacter ;
                       rdfs:comment "This property relates a literary text with a fictional character that is mentioned inside the narrative of that text"@en ;
                       rdfs:label "has fictional character"@en .


###  https://mangiafrangette.github.io/cosmo.owl#hasLiteraryValue
:hasLiteraryValue rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isLiteraryValueIn ;
                  rdfs:domain :literaryText ;
                  rdfs:range :interpretedConcept ;
                  rdfs:comment "This property relates a text with an interpreted concept recognised by Calvino's as a value or virtue of literature."@en ;
                  rdfs:label "has literary value"@en .


###  https://mangiafrangette.github.io/cosmo.owl#hasRhetoricalFigure
:hasRhetoricalFigure rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isUsedIn ;
                     rdfs:domain :literaryText ;
                     rdfs:range :rhetoricalFigure ;
                     rdfs:comment "This property relates a literary text that contains a rhetorical figure."@en ;
                     rdfs:label "has rhetorical figure"@en .


###  https://mangiafrangette.github.io/cosmo.owl#hasSource
:hasSource rdf:type owl:ObjectProperty ;
           owl:inverseOf :isSource ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :fictionalCharacter
                                       :interpretation
                                     )
                       ] ;
           rdfs:range :literaryText ;
           rdfs:comment "This property relates a fictional character or interpretation to the text where it comes from."@en ;
           rdfs:label "has source"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isAbsenceOf
:isAbsenceOf rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSubtractionOf ;
             rdfs:domain :heaviness ;
             rdfs:range :lightness ;
             rdfs:comment "This property states that heaviness is absence of lightness."@en ;
             rdfs:label "is absence of"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isAbsentFrom
:isAbsentFrom rdf:type owl:ObjectProperty ;
              owl:inverseOf :lacksOf ;
              rdfs:domain :lightness ;
              rdfs:range :literaryText ;
              rdfs:comment "This property shows when lightness is absent from a particular literary text."@en ;
              rdfs:label "is absent from"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isDenotedBy
:isDenotedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :interpretedConcept ;
             rdfs:range :rhetoricalFigure ;
             rdfs:comment "This property relates an interpreted concept to a rhetorical figure."@en ;
             rdfs:label "is denoted by"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isEmbodimentOf
:isEmbodimentOf rdf:type owl:ObjectProperty ;
                rdfs:domain :fictionalCharacter ;
                rdfs:range :interpretedConcept ;
                rdfs:comment "Relates a fictional character to the interpreted concept that it symbolizes"@en ;
                rdfs:label "is embodiment of"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isFictionalCharacterIn
:isFictionalCharacterIn rdf:type owl:ObjectProperty ;
                        rdfs:domain :fictionalCharacter ;
                        rdfs:range :literaryText ;
                        rdfs:comment "This property relates a fictional character that is present in a particular literary text."@en ;
                        rdfs:label "is fictional character in"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isLightenedBy
:isLightenedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :lightens ;
               rdfs:domain :heaviness ;
               rdfs:range :mutability ;
               rdfs:comment "This property describes the relation between heaviness and mutability since the latter is what changes heaviness into lightness."@en ;
               rdfs:label "is lightened by"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isLiteraryValueIn
:isLiteraryValueIn rdf:type owl:ObjectProperty ;
                   rdfs:domain :interpretedConcept ;
                   rdfs:range :literaryText ;
                   rdfs:comment "This property relates an interpreted concept recognised by Calvino's as a value or virtue of literature with a particular literary text."@en ;
                   rdfs:label "is literary value in"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isOppositeOf
:isOppositeOf rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :interpretedConcept ;
              rdfs:range :interpretedConcept ;
              rdfs:comment "This property states the opposition between heaviness and lightness"@en ;
              rdfs:label "is opposite of"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isSource
:isSource rdf:type owl:ObjectProperty ;
          rdfs:domain :literaryText ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :fictionalCharacter
                                     :interpretation
                                   )
                     ] ;
          rdfs:comment "This property relates a particular literary text with a fictional character that is present in it."@en ;
          rdfs:label "is source"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isSubtractionOf
:isSubtractionOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :lightness ;
                 rdfs:range :heaviness ;
                 rdfs:comment "This property states that lightness is related to heaviness because of a subtraction of weight."@en ;
                 rdfs:label "is subtraction of"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isUsedIn
:isUsedIn rdf:type owl:ObjectProperty ;
          rdfs:domain :rhetoricalFigure ;
          rdfs:range :literaryText ;
          rdfs:comment "This property relates a rhetorical figure with a particular literary text in which it is present."@en ;
          rdfs:label "is used in"@en .


###  https://mangiafrangette.github.io/cosmo.owl#lacksOf
:lacksOf rdf:type owl:ObjectProperty ;
         rdfs:domain :literaryText ;
         rdfs:range :lightness ;
         rdfs:comment "This property relates a literary text with lightness through a connection stating that the text lacks of lightness"@en ;
         rdfs:label "lacks of"@en .


###  https://mangiafrangette.github.io/cosmo.owl#lightens
:lightens rdf:type owl:ObjectProperty ;
          rdfs:domain :mutability ;
          rdfs:range :heaviness ;
          rdfs:comment "This property indicates the lightening force of mutability upon the category of heaviness."@en ;
          rdfs:label "lightens"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:DatatypeProperty ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( :fictionalCharacter
                                                                      :literaryText
                                                                    )
                                                      ] ;
                                          rdfs:range xsd:anyURI .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:DatatypeProperty ;
                                        rdfs:domain :literaryText ;
                                        rdfs:range xsd:string ;
                                        rdfs:comment "A name given to the resource"@en ;
                                        rdfs:isDefinedBy <http://purl.org/dc/elements/1.1> ;
                                        rdfs:label "Title"@en .


###  https://mangiafrangette.github.io/cosmo.owl#isLocatedInLine
:isLocatedInLine rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :textualPart ;
                 rdfs:domain :rhetoricalFigure ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Relates a rhetorical figure to a specific line of the textual reference"@en ;
                 rdfs:label "is located in line"@en .


###  https://mangiafrangette.github.io/cosmo.owl#textualPart
:textualPart rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :textualReference ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :fictionalCharacter
                                         :interpretation
                                         :literaryText
                                       )
                         ] ;
             rdfs:range xsd:string ;
             rdfs:comment "Relates a concept or agent to a fragment that includes their textual string reference"@en ;
             rdfs:label "has textual part"@en .


###  https://mangiafrangette.github.io/cosmo.owl#textualReference
:textualReference rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :fictionalCharacter
                                              :interpretation
                                              :literaryText
                                            )
                              ] ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Relates a concept or agent to a text that includes their textual string reference"@en ;
                  rdfs:label "has textual representation"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/Agent
<http://purl.org/dc/terms/Agent> rdf:type owl:Class ;
                                 owl:disjointWith <http://purl.org/vocab/frbr/core#Expression> ;
                                 rdfs:comment "A resource that acts or has the power to act"@en ;
                                 rdfs:isDefinedBy "http://purl.org/dc/terms/Agent" ;
                                 rdfs:label "Agent"@en .


###  http://purl.org/vocab/frbr/core#Concept
<http://purl.org/vocab/frbr/core#Concept> rdf:type owl:Class ;
                                          rdfs:comment "An abstract idea or notion"@en ;
                                          rdfs:isDefinedBy "http://purl.org/vocab/frbr/core#Concept" ;
                                          rdfs:label "Concept"@en .


###  http://purl.org/vocab/frbr/core#Expression
<http://purl.org/vocab/frbr/core#Expression> rdf:type owl:Class ;
                                             rdfs:comment "A realization of a single work usually in a physical form."@en ;
                                             rdfs:isDefinedBy "http://purl.org/vocab/frbr/core#Expression"@en ;
                                             rdfs:label "Expression"@en .


###  http://purl.org/vocab/frbr/core#Person
<http://purl.org/vocab/frbr/core#Person> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://purl.org/dc/terms/Agent> ;
                                         rdfs:comment "A living or dead individual"@en ;
                                         rdfs:isDefinedBy "http://purl.org/vocab/frbr/core#Person" ;
                                         rdfs:label "Person"@en .


###  https://mangiafrangette.github.io/cosmo.owl#brightness
:brightness rdf:type owl:Class ;
            rdfs:subClassOf :lightness ;
            rdfs:comment "The visual luminosity expressed in a literary text"@en ;
            rdfs:label "Brightness"@en ;
            rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/05025269-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#darkness
:darkness rdf:type owl:Class ;
          rdfs:subClassOf :heaviness ;
          rdfs:comment "Absence of light"@en ;
          rdfs:label "Darkness"@en ;
          rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/14007000-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#dustiness
:dustiness rdf:type owl:Class ;
           rdfs:subClassOf :lightness ;
           rdfs:comment "Quality of things that have nature, aspect, appearence of fine dust and are therefore discontinuous and fragmentary."@en ;
           rdfs:label "Dustiness"@en .


###  https://mangiafrangette.github.io/cosmo.owl#equality
:equality rdf:type owl:Class ;
          rdfs:subClassOf :lightness ;
          rdfs:comment "Quality of things that are put on the same level"@en ;
          rdfs:label "Equality"@en .


###  https://mangiafrangette.github.io/cosmo.owl#fictionalCharacter
:fictionalCharacter rdf:type owl:Class ;
                    rdfs:subClassOf <http://purl.org/vocab/frbr/core#Person> ;
                    rdfs:comment "An imaginary person represented in a work of literary fiction"@en ;
                    rdfs:label "Fictional character"@en ;
                    rdfs:seeAlso "http://dbpedia.org/ontology/FictionalCharacter" .


###  https://mangiafrangette.github.io/cosmo.owl#heaviness
:heaviness rdf:type owl:Class ;
           rdfs:subClassOf :interpretedConcept ;
           owl:disjointWith :lightness ;
           rdfs:comment "A concept to represent the weight, the inertia, the opacity of the world and the writing"@en ;
           rdfs:label "Heaviness"@en .


###  https://mangiafrangette.github.io/cosmo.owl#identity
:identity rdf:type owl:Class ;
          rdfs:subClassOf :rhetoricalFigure ;
          rdfs:comment "Identity refers to when a figure uses two or more identical elements. e.g., Ploce: O villain, villain, smiling, damned villain!(Hamlet [5])." ;
          rdfs:label "Identity" .


###  https://mangiafrangette.github.io/cosmo.owl#inertia
:inertia rdf:type owl:Class ;
         rdfs:subClassOf :heaviness ;
         rdfs:comment "The inability to act as expressed in literary texts"@en ;
         rdfs:label "Inertia"@en ;
         rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/04642988-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#interpretation
:interpretation rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.org/vocab/frbr/core#Concept> ;
                rdfs:comment "This class defines the concepts involved when interpreting a literary text"@en ;
                rdfs:label "Interpretation"@en .


###  https://mangiafrangette.github.io/cosmo.owl#interpretedConcept
:interpretedConcept rdf:type owl:Class ;
                    rdfs:subClassOf :interpretation ;
                    owl:disjointWith :rhetoricalFigure ;
                    rdfs:comment "A concept that results from interpreting a literary text"@en ;
                    rdfs:label "Interpreted concept"@en .


###  https://mangiafrangette.github.io/cosmo.owl#lightness
:lightness rdf:type owl:Class ;
           rdfs:subClassOf :interpretedConcept ;
           rdfs:comment "Literary value or virtue of a literary text reached by removing weight from the structure of stories and from language."@en ;
           rdfs:label "Lightness"@en ;
           rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/05035866-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#literaryText
:literaryText rdf:type owl:Class ;
              rdfs:subClassOf <http://purl.org/vocab/frbr/core#Expression> ;
              rdfs:comment "A textual expression of a work of literature"@en ;
              rdfs:label "Literary text"@en .


###  https://mangiafrangette.github.io/cosmo.owl#movement
:movement rdf:type owl:Class ;
          rdfs:subClassOf :lightness ;
          rdfs:comment "A movement from a place or location to another in literary texts"@en ;
          rdfs:label "Movement"@en ;
          rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/00280679-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#mutability
:mutability rdf:type owl:Class ;
            rdfs:subClassOf :lightness ;
            rdfs:comment "Quality of everything that can be transformed into something else"@en ;
            rdfs:label "Mutability"@en ;
            rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/04748551-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#omission
:omission rdf:type owl:Class ;
          rdfs:subClassOf :rhetoricalFigure ;
          rdfs:comment "Omission refers to when a figure omits expected elements. e.g., Asyndeton (which omits the expected conjunction, “and”): I came, I saw, I conquered."@en ;
          rdfs:label "Omission"@en .


###  https://mangiafrangette.github.io/cosmo.owl#opposition
:opposition rdf:type owl:Class ;
            rdfs:subClassOf :rhetoricalFigure ;
            rdfs:comment "Opposition refers to when a figure oppose two structures or concepts. e.g., Oxymoron: A wanton modesty. Proud humility."@en ;
            rdfs:label "Opposition"@en .


###  https://mangiafrangette.github.io/cosmo.owl#repetition
:repetition rdf:type owl:Class ;
            rdfs:subClassOf :rhetoricalFigure ;
            rdfs:comment "Repetition refers to when a figure uses repetition of sounds (consonants, vowels, or syllables), words, syntax (phrases or clauses) or semantics (concepts). e.g., Rhyme–Hickory Dickory Dock. The mouse ran up the clock."@en ;
            rdfs:label "Repetition"@en .


###  https://mangiafrangette.github.io/cosmo.owl#rhetoricalFigure
:rhetoricalFigure rdf:type owl:Class ;
                  rdfs:subClassOf :interpretation ;
                  rdfs:comment "The class of some common strategies employed by rhetorical figures"@en ;
                  rdfs:label "Rhetorical figure"@en ;
                  rdfs:seeAlso "https://www.researchgate.net/publication/220961451_Toward_an_ontology_of_rhetorical_figures" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :rhetoricalFigure ;
   owl:annotatedProperty rdfs:seeAlso ;
   owl:annotatedTarget "https://www.researchgate.net/publication/220961451_Toward_an_ontology_of_rhetorical_figures" ;
   rdfs:seeAlso "DOI: 10.1145/1878450.1878471"
 ] .


###  https://mangiafrangette.github.io/cosmo.owl#series
:series rdf:type owl:Class ;
        rdfs:subClassOf :rhetoricalFigure ;
        rdfs:comment "Series refers to when a figure establishes a series (through words or concepts). e.g., Abecedarian: Adorable, beautiful, charming, delightful, exciting, fantastic–you run the gamut from A to Z [1]."@en ;
        rdfs:label "Series"@en .


###  https://mangiafrangette.github.io/cosmo.owl#similarity
:similarity rdf:type owl:Class ;
            rdfs:subClassOf :rhetoricalFigure ;
            rdfs:comment "Similarity (partial-identity) refers to when a figure uses resemblance of a concept. e.g., Simile: My love is like a red, red rose."@en ;
            rdfs:label "Similarity"@en .


###  https://mangiafrangette.github.io/cosmo.owl#stillness
:stillness rdf:type owl:Class ;
           rdfs:subClassOf :heaviness ;
           rdfs:comment "A lack of physical or literary motion as represented in literary texts"@en ;
           rdfs:label "Stillness"@en ;
           rdfs:seeAlso "http://wordnet-rdf.princeton.edu/id/14029664-n"@en .


###  https://mangiafrangette.github.io/cosmo.owl#symmetry
:symmetry rdf:type owl:Class ;
          rdfs:subClassOf :rhetoricalFigure .


#################################################################
#    Individuals
#################################################################

###  https://mangiafrangette.github.io/cosmo.owl#Ermione
:Ermione rdf:type owl:NamedIndividual ,
                  :fictionalCharacter ;
         :hasSource :literaryText2 ;
         :isFictionalCharacterIn :literaryText2 ;
         :textualPart "Ermione" .


###  https://mangiafrangette.github.io/cosmo.owl#Movesi
:Movesi rdf:type owl:NamedIndividual ,
                 :movement ;
        :hasSource :literaryText1 ;
        :isLiteraryValueIn :literaryText1 ;
        :isOppositeOf :stanco ,
                      :traendo ;
        :textualPart "Movesi il vecchierel canut e bianco" .


###  https://mangiafrangette.github.io/cosmo.owl#Wind
:Wind rdf:type owl:NamedIndividual ,
               :fictionalCharacter ;
      :hasSource :literaryText4 ;
      :isFictionalCharacterIn :literaryText4 ;
      :textualPart "The Wind" .


###  https://mangiafrangette.github.io/cosmo.owl#alba
:alba rdf:type owl:NamedIndividual ,
               :brightness ;
      :hasSource :literaryText3 ;
      :isLiteraryValueIn :literaryText3 ;
      :textualPart "s’apriva l’alba." ;
      rdfs:label "alba" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora1
:anaphora1 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :denotes :silvani ,
                    :volti ;
           :hasSource :literaryText2 ;
           :isUsedIn :literaryText2 ;
           :isLocatedInLine "8, 10, 12, 14, 20, 22" ;
           :textualPart "Piove [...]/Piove [...]/Piove [...]/Piove [...]/Piove [...]" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora10
:anaphora10 rdf:type owl:NamedIndividual ,
                     :repetition ;
            :denotes :pietra ;
            :hasSource :literaryText3 ;
            :textualPart "terra[...]/terra[...]/terra[...]" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora11
:anaphora11 rdf:type owl:NamedIndividual ,
                     :repetition ;
            :denotes :cantina ;
            :hasSource :literaryText3 ;
            :textualPart "sei [...]/sei [...]/sei [...]" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora12
:anaphora12 rdf:type owl:NamedIndividual ,
                     :repetition ;
            :denotes :buia ,
                     :camera ;
            :hasSource :literaryText3 ;
            :textualPart "Sei buia" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora2
:anaphora2 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :hasSource :literaryText2 ;
           :isUsedIn :literaryText2 ;
           :textualPart "ieri/t'illuse, oggi m'illude" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora3
:anaphora3 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :hasSource :literaryText2 ;
           :isUsedIn :literaryText2 ;
           :isLocatedInLine "47, 48, 49, 50" ;
           :textualPart "ha un suono, e il mirto/altro suono, e il ginepro/altro ancóra, stromenti/diversi" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora4
:anaphora4 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :hasSource :literaryText2 ;
           :isUsedIn :literaryText2 ;
           :textualPart "Ascolta, ascolta" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora5
:anaphora5 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :hasSource :literaryText2 ;
           :isUsedIn :literaryText2 ;
           :isLocatedInLine "77, 78" ;
           :textualPart "trema, si spegne/trema, si spegne" .


###  https://mangiafrangette.github.io/cosmo.owl#anaphora6
:anaphora6 rdf:type owl:NamedIndividual ,
                    :repetition ;
           :hasSource :literaryText2 ;
